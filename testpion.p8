#include utils.lua

function printtable(t, indent)
	if (indent == nil) indent = 0
	
	local s=""
	for i=0,indent do
		s=s.." "
	end
	for k,v in pairs(t) do
	 if type(v) == "table" then
	  print(s..k..": {")
	  printtable(v, indent+1)
	  print(s.."}")
	 else
	  print(s..k..": "..v)
	 end
	end
end

function _init()
 local s = [[
 	a= b
 	b= cde
 	f= {
 		1= pee
 		2= poop
 	}
 	g= { str= ing }
 ]]
 printtable(parse_pion(s))
end
